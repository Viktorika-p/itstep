class Coffe:
    def __init__(self, size_cup, coffe_name, milk):
        self.size_cup = size_cup
        self.coffe_name = coffe_name
        self.milk = milk

    def get_cup_size(self, size_cup):
        return size_cup

    def coffe_name(self, coffe_name):
        return coffe_name

    def milk(self, milk):
        self.milk = milk
        while True:
            return ("Milk in the cofee")
        else:
            print("coffe without milk")

if __name__ == "__main__":
    test = Coffe(2, arrabika, milk)
    for elem in test:
        print(elem)
