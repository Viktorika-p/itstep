def add_new_spell(spells_list: set, new_spell: str) -> bool:
    while True:
        new_spell = input("Enter new spell ")
        if new_spell() in [spells_list]:
            return spells_list.union(new_spell) and print ("True")
        else:
            print("Falce")
    
 ron_spells = {"Accio", "Wingardium Leviosa", "Alohomora", "Expelliarmus"}
 harry_spells = {"Accio", "Wingardium Leviosa", "Expelliarmus", "Expecto patronum"}
