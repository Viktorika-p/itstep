#1 
import pickle
shopping_list_example = [
    {
        "bread": 1.2,
        "milk": 1.6,
        "potato": 0.4,
        "meat": 2.4
    },
    {
        "bread": 1.2,
        "milk": 1.6,
        "potato": 0.4,
        "eggs": 0.4,
        "fish": 2.4
    }
]

with open("shopping_list.pkl", 'rb') as pkl_list:
    print(pickle.dumps(shopping_list_example))
    pickle.dumps(shopping_list_example, pkl_list)
